{
  "autoHide.autoHidePanel": false,
  "autoHide.autoHideSideBar": true,
  "autoHide.hideOnOpen": false,
  "autoHide.sideBarDelay": 300,

  "blockhighlight.background": ["127", "127", "127", ".050"],

  "bracket-pair-colorizer-2.colors": [
    "Gold",
    "Orchid",
    "LightSkyBlue"
  ],
  "bracket-pair-colorizer-2.forceIterationColorCycle": true,
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.showBracketsInRuler": true,

  "dimmer.delay": 600,
  "dimmer.enabled": true,
  "dimmer.opacity": 90,

  "editor.codeActionsOnSave": [
    "source.fixAll.format",
    "source.fixAll.eslint"
  ],
  "editor.cursorSmoothCaretAnimation": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "FiraCode-Retina",
  "editor.fontLigatures": true,
  "editor.fontSize": 23,
  "editor.fontWeight": "normal",
  "editor.formatOnSave": true,
  "editor.minimap.showSlider": "always",
  "editor.minimap.size": "fit",
  "editor.overviewRulerBorder": false,
  "editor.renderWhitespace": "all",
  "editor.showFoldingControls": "always",
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,

  "eslint.format.enable": true,
  "eslint.nodeEnv": "dev",
  "eslint.packageManager": "yarn",

  "extensions.autoCheckUpdates": true,
  "extensions.autoUpdate": true,

  "files.trimTrailingWhitespace": true,

  "git-graph.openToTheRepoOfTheActiveTextEditorDocument": true,

  "git.autofetch": true,
  "git.enableSmartCommit": true,

  "highlightLine.borderColor": "black",
  "highlightLine.borderWidth": "10",

  "indentRainbow.colors": [
    "rgba(64,64,64,0.23)",
    "rgba(96,96,96,0.23)",
    "rgba(128,128,128,0.23)",
    "rgba(160,160,160,0.23)",
    "rgba(192,192,192,0.23)",
    "rgba(224,224,224,0.23)",
    "rgba(255,255,255,0.23)",
    "rgba(224,224,224,0.23)",
    "rgba(192,192,192,0.23)",
    "rgba(160,160,160,0.23)",
    "rgba(128,128,128,0.23)",
    "rgba(96,96,96,0.23)",
    "rgba(64,64,64,0.23)"
  ],
  "indentRainbow.updateDelay": 1000,

  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.updateImportsOnFileMove.enabled": "always",

  "js/ts.implicitProjectConfig.checkJs": true,

  "json.format.enable": false,

  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.files.associations": {
    "index.tsx": "settings"
  },
  "material-icon-theme.folders.associations": {
    "analytics": "review",
    "assets": "layout",
    "automata": "hook",
    "cycles": "generator",
    "dev": "tools",
    "screens": "app",
    "spawn": "global",
    "types": "mappings"
  },

  "prettier.arrowParens": "avoid",
  "prettier.bracketSpacing": true,
  "prettier.endOfLine": "crlf",
  "prettier.jsxBracketSameLine": false,
  "prettier.jsxSingleQuote": true,
  "prettier.packageManager": "yarn",
  "prettier.printWidth": 64,
  "prettier.quoteProps": "consistent",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "none",
  "prettier.useEditorConfig": false,
  "prettier.useTabs": false,

  "separators.classes.borderStyle": "solid",
  "separators.classes.borderWidth": 1,
  "separators.constructors.borderStyle": "solid",
  "separators.constructors.borderWidth": 1,
  "separators.enabledSymbols": [
    "Classes",
    "Constructors",
    "Enums",
    "Functions",
    "Interfaces",
    "Methods",
    "Namespaces"
  ],
  "separators.enums.borderStyle": "solid",
  "separators.enums.borderWidth": 1,
  "separators.functions.borderStyle": "solid",
  "separators.functions.borderWidth": 1,
  "separators.interfaces.borderStyle": "solid",
  "separators.interfaces.borderWidth": 1,
  "separators.methods.borderStyle": "solid",
  "separators.methods.borderWidth": 1,
  "separators.namespaces.borderStyle": "solid",
  "separators.namespaces.borderWidth": 1,

  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.lineHeight": 1.3,

  "todo-tree.filtering.ignoreGitSubmodules": true,
  "todo-tree.filtering.includedWorkspaces": ["/src"],
  "todo-tree.general.statusBar": "tags",
  "todo-tree.general.tagGroups": {
    "TODO": ["TODO"],
    "FIXME": ["FIXME"],
    "BUG": ["BUG"]
  },
  "todo-tree.general.tags": ["NOTE", "TODO", "FIXME", "BUG"],
  "todo-tree.highlights.customHighlight": {
    "BUG": {
      "foreground": "rgba(0,0,0,0.5)",
      "background": "rgba(255,128,0,0.5)",
      "borderRadius": "5px",
      "icon": "bug",
      "iconColour": "rgba(255,128,0,0.5)"
    },
    "FIXME": {
      "foreground": "rgba(255,128,0,5)",
      "background": "rgba(255,0,0,0.5)",
      "borderRadius": "5px",
      "icon": "flame",
      "iconColour": "rgba(255,128,0,0.5)"
    },
    "NOTE": {
      "foreground": "rgba(0,0,0,0.8)",
      "background": "rgba(255,128,0,0.5)",
      "borderRadius": "5px",
      "icon": "check-circle-fill",
      "iconColour": "rgba(255,128,0,0.5)"
    },
    "TODO": {
      "type": "whole-line",
      "foreground": "rgba(0,0,0,0.8)",
      "background": "rgba(64, 146, 222,0.5)",
      "borderRadius": "10px",
      "icon": "check-circle-fill",
      "iconColour": "rgba(255,128,0,0.5)"
    }
  },
  "todo-tree.highlights.defaultHighlight": {
    "background": "rgba(255,128,0,0.5)",
    "borderRadius": "5px",
    "foreground": "rgba(255,128,0,0.5)",
    "gutterIcon": true,
    "iconColour": "rgba(255,128,0,0.5)",
    "rulerLane": "none",
    "type": "text-and-comment"
  },
  "todo-tree.tree.hideTreeWhenEmpty": true,
  "todo-tree.tree.showScanModeButton": false,

  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.updateImportsOnFileMove.enabled": "always",

  "window.autoDetectHighContrast": false,
  "window.nativeFullScreen": true,
  "window.nativeTabs": true,
  "window.title": "${activeEditorShort}${separator}${rootName}",
  "window.titleBarStyle": "native",
  "window.zoomLevel": 0,

